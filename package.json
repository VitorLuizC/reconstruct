{
  "name": "reconstruct",
  "version": "0.1.3",
  "description": "Maps object into a new one using an anonymous function.",
  "cdn": "dist/reconstruct.js",
  "main": "dist/reconstruct.cjs.js",
  "unpkg": "dist/reconstruct.js",
  "types": "dist/reconstruct.d.ts",
  "module": "dist/reconstruct.es.js",
  "browser": "dist/reconstruct.js",
  "jsdelivr": "dist/reconstruct.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:typescript": "tsc src/reconstruct.ts --outDir dist -t es2015 -d",
    "build:bili": "bili dist/reconstruct.js --formats cjs,es,umd --js buble --banner",
    "build": "npm run build:typescript && npm run build:bili",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitorLuizC/reconstruct.git"
  },
  "keywords": [
    "reconstruct",
    "object-map",
    "functional-programming"
  ],
  "author": {
    "url": "https://vitorluizc.github.io/",
    "name": "Vitor Luiz Cavalcanti",
    "email": "vitorluizc@outlook.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VitorLuizC/reconstruct/issues"
  },
  "homepage": "https://github.com/VitorLuizC/reconstruct#readme",
  "dependencies": {
    "object.assign": "^4.1.0",
    "object.entries": "^1.0.4"
  },
  "devDependencies": {
    "bili": "^2.2.7",
    "typescript": "^2.7.2"
  }
}
