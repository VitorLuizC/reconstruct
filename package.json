  "dependencies": {
    "nano-assign": "^1.0.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "ava-ts": "^0.25.2",
    "bili": "^3.3.0",
    "rollup-plugin-typescript2": "^0.17.2",
    "ts-node": "^7.0.1",
